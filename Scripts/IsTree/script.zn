//! zinc
library IsTree {
    private unit Dummy = CreateUnit(
        Player(PLAYER_NEUTRAL_PASSIVE), 'hpea', 0, 0, 0
    );
    
    public function IsTree(destructable whichDestructible) -> boolean {
        return 
            IssueTargetOrderById(Dummy, 852185, whichDestructible) &&
            IssueImmediateOrderById(Dummy, 851972);
    }

    private function onInit() {
        UnitAddAbility(Dummy, 'Aloc');
        ShowUnit(Dummy, false);
    }
}
//! endzinc
